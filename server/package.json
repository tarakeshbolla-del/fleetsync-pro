{
    "name": "fleetsync-server",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "node node_modules/typescript/bin/tsc",
        "start": "node dist/index.js",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "node node_modules/prisma/build/index.js migrate dev",
        "db:seed": "tsx prisma/seed.ts"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.11.16",
        "@types/node-cron": "^3.0.11",
        "@types/pdfkit": "^0.17.4",
        "@types/uuid": "^9.0.8",
        "tsx": "^4.7.0"
    },
    "dependencies": {
        "@prisma/client": "^5.10.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "csv-parse": "^5.5.3",
        "dotenv": "^16.4.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.3",
        "pdfkit": "^0.17.2",
        "prisma": "^5.10.0",
        "typescript": "^5.3.3",
        "uuid": "^9.0.1"
    },
    "prisma": {
        "seed": "node --loader tsx/esm prisma/seed.ts"
    }
}